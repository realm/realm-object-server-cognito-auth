{
  "name": "realm-object-server-cognito-auth",
  "version": "1.0.0",
  "description": "Cognito authentication provider for Realm Object Server",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist coverage realm-object-server test-results.xml",
    "prepack": "npm run build",
    "build": "rm -rf dist && tsc",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint-fix": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "test": "nyc mocha './src/**/*.spec.ts'",
    "docker-build": "./scripts/docker-build.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/realm/realm-object-server-cognito-auth.git"
  },
  "keywords": [
    "Realm",
    "authentication",
    "Realm",
    "Object",
    "Server"
  ],
  "author": "Realm",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/realm/realm-object-server-cognito-auth/issues"
  },
  "homepage": "https://github.com/realm/realm-object-server-cognito-auth#readme",
  "dependencies": {
    "jsonwebtoken": "^8.1.0",
    "pem-jwk": "^1.5.1",
    "superagent": "^3.8.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.0",
    "@types/chai-as-promised": "0.0.31",
    "@types/jsonwebtoken": "^7.2.3",
    "@types/mocha": "^2.2.44",
    "@types/sinon": "^2.3.3",
    "@types/superagent": "^3.5.6",
    "chai": "^3.5.0",
    "chai-as-promised": "^7.1.1",
    "fs-extra": "^4.0.2",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.1",
    "mocha-jenkins-reporter": "^0.3.9",
    "nyc": "^10.3.2",
    "realm-object-server": "^2.0.13",
    "sinon": "^3.2.1",
    "source-map-support": "^0.4.15",
    "superagent-mock": "^3.6.0",
    "ts-node": "^3.0.4",
    "tslint": "^5.2.0",
    "typescript": "^2.4.2"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts",
      "node_modules",
      "dist"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov",
      "json"
    ],
    "sourceMap": true,
    "instrument": true,
    "all": true
  }
}
